function Common_procAppend(obj) {
    if (!obj) return;

    procAppend_MonsterBlood(obj); // 怪物血条
    AddMonsterApend(obj); // 怪物异常状态
}


function procAppend_Swordman(obj) {
    if (!obj) return;

    Common_procAppend(obj);
}



