function onAfterSetState_atgunner_throw(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB9 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB10 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB11)
{
 if (!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8) return;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.getThrowIndex();
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.getThrowState();
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.sq_IsMyControlObject())
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 == 1)
 switch (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1)
 
{
 case 79:
 case 80:
 case 81:
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 = onGetMyPassiveObject_My(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8 , 24376 , SKILL_ATGUNNER_GSPFALCON , 1);
