 else
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.getVar().push_vector(0);
 
}
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB9 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.get_vector(1);
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB10 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.get_vector(3);
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB11 = getArcMoveValue(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB9 + sq_GetAccel(0 , 50 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5 , true) , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB10 , 100).tointeger();
 sq_setCurrentAxisPos(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12 , 2 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB11);
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 >= Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5)
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.sq_IsMyControlObject())
 
{
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.sq_IntVectClear();
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.sq_IntVectPush(1);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.sq_IntVectPush(0);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.sq_IntVectPush(1);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.sq_AddSetStatePacket(STATE_JUMP , STATE_PRIORITY_USER , true);
 
}
 break;
 
}
}
