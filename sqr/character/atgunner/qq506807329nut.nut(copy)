 sq_SetMyShake(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 , 6 , 240);
 sq_flashScreen(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 , 0 , 40 , 0 , 102 , sq_RGB(255 , 255 , 255) , GRAPHICEFFECT_NONE , ENUM_DRAWLAYER_BOTTOM);
 
}
 break;
 case 2:
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 == 1)
