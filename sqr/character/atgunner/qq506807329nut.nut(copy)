}
 else if (sq_IsKeyDown(OPTION_HOTKEY_MOVE_UP , ENUM_SUBKEY_TYPE_ALL)
 || (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 == ENUM_DIRECTION_LEFT && sq_IsKeyDown(OPTION_HOTKEY_MOVE_RIGHT , ENUM_SUBKEY_TYPE_ALL))
 || (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 == ENUM_DIRECTION_RIGHT && sq_IsKeyDown(OPTION_HOTKEY_MOVE_LEFT , ENUM_SUBKEY_TYPE_ALL)))
 
