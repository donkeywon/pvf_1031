 case 2:
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.sq_IntVectClear();
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.sq_IntVectPush(1); 
