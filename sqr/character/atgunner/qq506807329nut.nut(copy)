 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 = 0;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1.getMyPassiveObjectCount(24376);
