function onProc_atgunner_pt_15(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12)
{
 if (!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12) return;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.getSkillSubState();
 switch (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1)
 
{
 case 3:
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB12.getVar();
