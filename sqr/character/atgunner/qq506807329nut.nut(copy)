function setEnableCancelSkill_ATGunner(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3)
{
 if(!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2)
 return false;
 
 if(!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.isMyControlObject())
 return false;
 if(!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3)
 return true;
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_PLASMABOOST , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_FSC_7 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_PT_15 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_OPERATIONRAZE , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_SUPPRESSINGFIRE , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_KILLPOINT , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_CHAINDEVIDER , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_BLOODNCHAIN , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_GRAVITYGRENADE , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_PISTOLCARBINE , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_OPENFIRE , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_G96THERMOBARICGRANADE , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_DDAY , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_HS12 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_G4 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1 = onGetMyPassiveObject_My(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 , 24376 , SKILL_ATGUNNER_GSPFALCON , 1);
 if(!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1)
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_GSPFALCON , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.setSkillCommandEnable(SKILL_ATGUNNER_MAGNETICFIELD , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
