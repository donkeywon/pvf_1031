function onProcCon_atgunner_plasmaboost(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4)
{
 if (!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4) return;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.getSkillSubState();
 switch (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1)
 
{
 case 1:
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.getVar();
 
 if (!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.isDownSkillLastKey() || Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.sq_GetStateTimer() >= Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.get_vector(0))
 
{
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.sq_IntVectClear();
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.sq_IntVectPush(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1 + 1); 
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.sq_AddSetStatePacket(STATE_ATGUNNER_PLASMABOOST , STATE_PRIORITY_USER , true);
 
}
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.getVar().getBool(0);
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 == false)
 
{
 
 if (sq_IsKeyDown(OPTION_HOTKEY_MOVE_UP , ENUM_SUBKEY_TYPE_ALL)
 || sq_IsKeyDown(OPTION_HOTKEY_MOVE_LEFT , ENUM_SUBKEY_TYPE_ALL)
 || sq_IsKeyDown(OPTION_HOTKEY_MOVE_DOWN , ENUM_SUBKEY_TYPE_ALL)
 || sq_IsKeyDown(OPTION_HOTKEY_MOVE_RIGHT , ENUM_SUBKEY_TYPE_ALL))
 
{
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.getVar().setBool(0 , true);
 sq_BinaryStartWrite();
 sq_BinaryWriteDword(1);
 sq_SendChangeSkillEffectPacket(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 , SKILL_ATGUNNER_PLASMABOOST);
 
}
 
}
 else if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 == true)
 
{
 
 if (!sq_IsKeyDown(OPTION_HOTKEY_MOVE_UP , ENUM_SUBKEY_TYPE_ALL)
 && !sq_IsKeyDown(OPTION_HOTKEY_MOVE_LEFT , ENUM_SUBKEY_TYPE_ALL)
 && !sq_IsKeyDown(OPTION_HOTKEY_MOVE_DOWN , ENUM_SUBKEY_TYPE_ALL)
 && !sq_IsKeyDown(OPTION_HOTKEY_MOVE_RIGHT , ENUM_SUBKEY_TYPE_ALL))
 
{
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.getVar().setBool(0 , false);
 sq_BinaryStartWrite();
 sq_BinaryWriteDword(2);
 sq_SendChangeSkillEffectPacket(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 , SKILL_ATGUNNER_PLASMABOOST);
 
}
 
}
 break;
 
}
}
