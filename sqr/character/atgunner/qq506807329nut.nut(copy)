 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3.getVar().push_vector(1);
 
}
 else
 
{
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3.getVar().push_vector(2);
 
}
 
}
 break;
 case 1:
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 == 1)
 
{
