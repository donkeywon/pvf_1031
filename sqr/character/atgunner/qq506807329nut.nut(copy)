function onKeyFrameFlag_atgunner_fsc_7(obj , flagIndex)
{
 if (!obj) return false;
 if (flagIndex == 1)
 
{
 obj.getVar().push_vector( -20);
 obj.getVar().push_vector(obj.getXPos());
 if (obj.isMyControlObject())
 
{
 obj.sq_StartWrite();
 obj.sq_WriteDword(SKILL_ATGUNNER_FSC_7);
 obj.sq_WriteDword(obj.sq_GetBonusRateWithPassive(SKILL_ATGUNNER_FSC_7 , STATE_ATGUNNER_FSC_7 , 0 , 1.0));
 obj.sq_WriteDword(obj.sq_GetBonusRateWithPassive(SKILL_ATGUNNER_FSC_7 , STATE_ATGUNNER_FSC_7 , 1 , 1.0));
 obj.sq_WriteDword(obj.sq_GetLevelData(SKILL_ATGUNNER_FSC_7 , 2 , sq_GetSkillLevel(obj , SKILL_ATGUNNER_FSC_7)));
 obj.sq_SendCreatePassiveObjectPacket(24376 , 0 , 139 , 0 , 84);
 sq_SetMyShake(obj , 5 , 150);
 
}
 
}
 return true;
}
