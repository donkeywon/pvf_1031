 return true;
}
function drawCustomUI_ATGunner(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8)
{
 if(!Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.isMyControlObject())
 return;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1 = CNSquirrelAppendage.sq_GetAppendage(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8 , "character/atgunner/appendage/ap_cutin_ani.nut");
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1 && Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1.isValid())
 
{
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1.sq_GetSkillIndex();
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 = -1;
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1.getVar("drawui").size_vector() <= 0)
 
{
 
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.getDirection();
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1.getVar("drawui").clear_vector();
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1.getVar("drawui").push_vector(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3);
 
}
 else
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB1.getVar("drawui").get_vector(0);
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 = null;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5 = 0;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB6 = 0;
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7 = 1.0;
 switch (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2)
 
{
 case STATE_ATGUNNER_OPERATIONRAZE:
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.getVar().GetAnimationMap("atgunner_stormtrooper" , "sqr/js60_qq506807329/cutinanimation/atgunner_stormtrooper.ani");
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5 = -266;
 break;
 case STATE_ATGUNNER_BLOODNCHAIN:
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.getVar().GetAnimationMap("atgunner_crimsonrose" , "sqr/js60_qq506807329/cutinanimation/atgunner_crimsonrose.ani");
 break;
 case STATE_ATGUNNER_DDAY:
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.getVar().GetAnimationMap("atgunner_freyja" , "sqr/js60_qq506807329/cutinanimation/atgunner_freyja.ani");
 break;
 case STATE_ATGUNNER_MECABOLT:
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8.getVar().GetAnimationMap("atgunner_optimus" , "sqr/js60_qq506807329/cutinanimation/atgunner_optimus.ani");
 break;
 
}
 if (sq_IsEnd(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4))
 
{
 sq_Rewind(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4);
 CNSquirrelAppendage.sq_RemoveAppendage(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB8 , "character/atgunner/appendage/ap_cutin_ani.nut");
 return;
 
}
 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3 == ENUM_DIRECTION_RIGHT)
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.setImageRateFromOriginal(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7);
 else
 
{
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4.setImageRateFromOriginal(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7 / -1.0 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5 = Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5 / -1 + 800;
 
}
 sq_AnimationProc(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4);
 sq_drawCurrentFrame(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB6 , false);
 
}
}

