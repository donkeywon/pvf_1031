 if (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4 != Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5)
 
{
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB6 = -1;
 switch (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5)
 
{
 case PASSIVEOBJ_SUB_STATE_0:Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB6 = 5; break;
 case PASSIVEOBJ_SUB_STATE_1:Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB6 = 4; break;
 case PASSIVEOBJ_SUB_STATE_2:Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB6 = 5; break;
 
}
 local Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7 = sq_GetGlobalIntVector();
 sq_IntVectorClear(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7);
 sq_IntVectorPush(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB6);
 sq_IntVectorPush(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB4);
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB3.addSetStatePacket(Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB5 , Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB7 , STATE_PRIORITY_AUTO , false , "");
 
}
 break;
 
}
