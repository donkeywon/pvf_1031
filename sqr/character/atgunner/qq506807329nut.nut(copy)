function onTimeEvent_atgunner_operationraze(obj , timeEventIndex , timeEventCount)
{
 if (!obj) return true;
 if(!obj.sq_IsMyControlObject()) return false;
 switch (timeEventIndex)
 
{
 case 0:
 
 obj.sq_StartWrite();
 obj.sq_WriteDword(SKILL_ATGUNNER_OPERATIONRAZE);
 obj.sq_WriteDword(4);
 obj.sq_WriteDword(obj.sq_GetBonusRateWithPassive(SKILL_ATGUNNER_OPERATIONRAZE , STATE_ATGUNNER_OPERATIONRAZE , 4 , 1.0));
 obj.sq_SendCreatePassiveObjectPacket(24376 , 0 , 110 , 0 , 35);
 break;
 
}
 return false;
}
