{
 switch (Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.getVar().get_vector(0))
 
{
 case 1:
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.sq_IntVectClear();
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.sq_IntVectPush(2); 
 Js60QQ506807329_ShouTu1000RMB_NutShouTu1500RMB2.sq_AddSetStatePacket(STATE_ATGUNNER_PT_15 , STATE_PRIORITY_USER , true);
