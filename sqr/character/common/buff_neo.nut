function ReadAheadBuffAni(obj)
{
	local jobBuffArray = AllJobBuffArray.rawget(sq_getJob(obj));
	local growTypeBuffArray = jobBuffArray.rawget(sq_getGrowType(obj));
	foreach(skillIndex, aniInfoArray in growTypeBuffArray)
	{
		if (sq_GetSkillLevel(obj, skillIndex) > 0)
		{
			createAnimationPooledObject(obj, aniInfoArray.rawget(0), true, 50000, 50000, 50000);
		}
	}
}

function AllJobBuffNeo(obj)
{
	local jobBuffArray = AllJobBuffArray.rawget(sq_getJob(obj));
	local currentSkillIndex = obj.getCurrentSkillIndex();
	local growTypeBuffArray = jobBuffArray.rawget(sq_getGrowType(obj));
	local aniInfo = growTypeBuffArray.rawget(currentSkillIndex);
	local aniInfoArray = aniInfo(obj);
	local aniPath = aniInfoArray.rawget(0);
	local xPos = aniInfoArray.rawget(1);
	local yPos = aniInfoArray.rawget(2);
	local aniDrawlayer = aniInfoArray.rawget(3);
	if (obj.sq_IsEnterSkill(currentSkillIndex) != -1)
	{
		local ani = sq_CreateAnimation("", aniPath);
		local pooledObj = sq_CreatePooledObject(ani, true);
		pooledObj = sq_SetEnumDrawLayer(pooledObj, aniDrawlayer);
		pooledObj.setCurrentPos(xPos, yPos, aniDrawlayer);
		sq_AddObject(obj, pooledObj, 2, false);
	}
}